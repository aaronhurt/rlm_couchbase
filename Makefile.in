TARGET      = @targetname@
SRCS        = rlm_couchbase.c
HEADERS     =
RLM_CFLAGS  = @couchbase_cflags@ -I/usr/local/include
RLM_LIBS    = @couchbase_ldflags@ -L/usr/local/lib -lcouchbase

